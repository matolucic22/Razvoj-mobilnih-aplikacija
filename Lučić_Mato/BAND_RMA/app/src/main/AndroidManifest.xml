<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="hr.etfos.mlucic.testzaband">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.GET_ACCOUNTS" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Activities.Activities.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>

            <meta-data
                android:name="android.app.searchable"
                android:value="@xml/searchable" />
        </activity>
        <activity android:name=".Activities.Activities.Kalendar" />
        <activity android:name=".Activities.Activities.Ispis" />
        <activity
            android:name=".Activities.Activities.Repertoar"
            android:launchMode="singleTop">
            <intent-filter>
                <action android:name="android.intent.action.SEARCH" />
            </intent-filter>
        </activity>
        <activity android:name=".Activities.Activities.Tekst_pjesme" />
        <activity android:name=".Activities.Activities.Greske.Greske" />
        <activity android:name=".Activities.Activities.Greske.Upis_gresaka.Upis_greske" />
        <activity android:name=".Activities.Activities.Greske.Ispis_greske.Ispis_gresaka" />
        <activity android:name=".Activities.Activities.Greske.Upis_gresaka.Upis_greske_final" />
        <activity android:name=".Activities.Activities.Greske.Ispis_greske.Ispis_greske_final"></activity>
    </application>

</manifest>